    "postcss": "^8.4.32",
    "postcss-cli": "^11.0.0",
    "postcss-color-mod-function": "^3.0.3",
    "postcss-conditionals": "^2.1.0",
    "postcss-easings": "^4.0.0",
    "postcss-for": "^2.1.1",
    "postcss-import": "^15.1.0",
    "postcss-inline-svg": "^6.0.0",
    "postcss-nested": "^6.0.1",
    "postcss-preset-env": "^9.3.0",
    "postcss-random": "^1.0.9",
